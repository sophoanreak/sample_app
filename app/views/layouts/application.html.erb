<!DOCTYPE html>
<html>
<head>
  <title>SampleApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

   <%= render 'layouts/materialize' %>

</head>
<body>

<%= render 'layouts/navbar' %>

<!--body-->
<div class="container">
   <p class="notice card blue-grey darken-1 card-content white-text"><%= notice%></p>
   <p class="alert card blue-grey darken-1 card-content white-text"><%= alert %></p>
<%= yield %>
</div>

<%= render 'layouts/footer' %>
<script>
$(function() {
  $(".button-collapse").sideNav();
});
</script>
</body>
</html>
